<Window x:Class="Sep.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sep"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Text="TextBox" TextWrapping="Wrap" Height="40" VerticalAlignment="Bottom" Margin="0,0,242,0"/>
        <Button Content="Button" Margin="0,0,190,0" RenderTransformOrigin="0.144,-0.166" HorizontalAlignment="Right" Width="52" Height="40" VerticalAlignment="Bottom"/>
        <ListBox Margin="0,0,190,40" Name="Messages" DisplayMemberPath="MessageView"/>
        <ListBox Margin="615,0,0,0" Name="Users"  DisplayMemberPath="UserName"/>

    </Grid>
</Window>
